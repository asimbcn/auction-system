Ext.define('MyApp.store.AddressTypeStore', {
    extend: 'Ext.data.Store',

    requires: [
        'MyApp.model.TypeModel',
        'Ext.data.proxy.Ajax',
        'Ext.data.reader.Json'
    ],

    constructor: function(cfg) {
        var me = this;
        cfg = cfg || {};
        me.callParent([Ext.apply({
            model: 'MyApp.model.TypeModel',
            storeId: 'AddressTypeStore',
            data: [
                {
                    TypeCode: 'P',
                    TypeDesc: 'स्थाई ठेगाना(Permanent)'
                },
                {
                    TypeCode: 'T',
                    TypeDesc: 'अस्थाई ठेगाना(Temporary)'
                },
                {
                    TypeCode: 'M',
                    TypeDesc: 'पत्राचार गर्ने ठेगाना(Mailing)'
                }
            ],
            proxy: {
                type: 'ajax',
                reader: {
                    type: 'json'
                }
            }
        }, cfg)]);
    }
});