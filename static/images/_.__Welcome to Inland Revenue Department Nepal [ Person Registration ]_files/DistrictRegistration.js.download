Ext.define('MyApp.store.DistrictRegistration', {
    extend: 'Ext.data.Store',

    requires: [
        'MyApp.model.District'
    ],

    constructor: function(cfg) {
        var me = this;
        cfg = cfg || {};
        me.callParent([Ext.apply({
            model: 'MyApp.model.District',
            storeId: 'DistrictRegistration',
            proxy: {
                type: 'ajax',
                //url: '../Handlers/Common/DistrictTypeHandler.ashx?method=LoadDistrictType&choose=Y&formToken='+Ext.get('frmToken').dom.innerHTML,
                reader: {
                    type: 'json',
                    root: 'root'
                }
            }
        }, cfg)]);
    }
});