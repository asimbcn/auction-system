Ext.define('MyApp.store.IndividualRelativeStore', {
    extend: 'Ext.data.Store',

    requires: [
        'MyApp.model.IndividualRelative',
        'Ext.data.proxy.Ajax',
        'Ext.data.reader.Json'
    ],

    constructor: function(cfg) {
        var me = this;
        cfg = cfg || {};
        me.callParent([Ext.apply({
            model: 'MyApp.model.IndividualRelative',
            storeId: 'IndividualRelativeStore',
            proxy: {
                type: 'ajax',
                reader: {
                    type: 'json'
                }
            }
        }, cfg)]);
    }
});