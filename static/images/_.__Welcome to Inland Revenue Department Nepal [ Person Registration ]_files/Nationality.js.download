Ext.define('MyApp.store.Nationality', {
    extend: 'Ext.data.Store',

    requires: [
        'MyApp.model.Nationality'
    ],

    constructor: function(cfg) {
        var me = this;
        cfg = cfg || {};
        me.callParent([Ext.apply({
            //autoLoad: false,
            model: 'MyApp.model.Nationality',
            storeId: 'Nationality',
            proxy: {
                type: 'ajax',
                //url: '../Handlers/Common/NationalityHandler.ashx?method=LoadNationality&formToken='+Ext.get('frmToken').dom.innerHTML,
                reader: {
                    type: 'json',
                    root: 'root'
                }
            }
        }, cfg)]);
    }
});