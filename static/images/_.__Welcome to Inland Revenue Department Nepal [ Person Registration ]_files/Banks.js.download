Ext.define('MyApp.store.Banks', {
    extend: 'Ext.data.Store',

    requires: [
        'MyApp.model.Banks'
    ],

    constructor: function (cfg) {
        var me = this;
        cfg = cfg || {};
        me.callParent([Ext.apply({
            model: 'MyApp.model.Banks',
            storeId: 'Banks',
            proxy: {
                type: 'ajax',
                reader: {
                    type: 'json',
                    root: 'root'
                }
            }
        }, cfg)]);
    }
});